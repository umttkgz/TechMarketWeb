@model List<Category>

<h1>Kategori Listesi</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>
                Kategori Adı
            </th>
            <th>
                Gösterim Sırası
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model.OrderBy(u => u.DisplayOrder))
        {
            <tr>
                <td>@obj.Name</td>
                <td>
                    @obj.DisplayOrder
                </td>
            </tr>
        }
    </tbody>
    
</table>